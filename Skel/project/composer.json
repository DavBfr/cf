{
	"name": "davbfr/skel",
	"version": "1.0",
	"description": "CoreFramework Project",
	"type": "project",
	"homepage": "@CF_URL@",
	"license": "MIT",
	"authors": [{
		"name": "@CF_AUTHOR@",
		"email": "@CF_EMAIL@",
		"homepage": "@CF_URL@",
		"role": "Developer"
	}],
	"require": {
		"davbfr/cf": "~@CF_VERSION@"
	},
	"require-dev": {
		"phpunit/phpunit": "~6"
	},
	"scripts": {
		"post-install-cmd": [
			"./setup install"
		],
		"post-update-cmd": [
			"./setup update"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"archive": {
		"exclude": [@EXCLUDES@]
	}
}
