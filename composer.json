		"phan/phan": "^1.0",
		"phpunit/phpunit": "~6"
	},
	"authors": [{
		"name": "David PHAM-VAN",
		"email": "dev.nfet.net@gmail.com",
		"homepage": "https://github.com/DavBfr/cf",
