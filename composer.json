	"require": {
		"php": ">=7.0.0",
		"phpmailer/phpmailer": "~5.2",
		"leafo/lessphp": "0.5.0",
		"tedivm/jshrink": "~1.0",
		"ext-json": "*",
		"ext-PDO": "*",
		"ext-mbstring": "*",
